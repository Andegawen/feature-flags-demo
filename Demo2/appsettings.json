{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FeatureManagement": {
    "BetaUser": false,
    "Cookie": {
      "EnabledFor": [
        {
          "Name": "CookieFeatureFilter",
          "Parameters": {
            "CookieName": "member",
            "CookieValue": "1"
          }
        }
      ]
    }
  },
  "AllowedHosts": "*"
}